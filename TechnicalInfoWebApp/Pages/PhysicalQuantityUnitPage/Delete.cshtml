@page
@model TechnicalInfoWebApp.Pages.PhysicalQuantityUnitPage.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>حذف</h1>

<h3>آیا از حذف فایل مطمئن می‌باشید؟</h3>
<div>
    <h4>واحد کمیت فیزیکی</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            کمیت فیزیکی
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TblPhysicalQuantityUnit.FldPhysicalQuantity.FldPhysicalQuantityTxt)
        </dd>
        <dt class="col-sm-2">
            واحد
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TblPhysicalQuantityUnit.FldUnit.FldUnitName)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="TblPhysicalQuantityUnit.FldPhysicalQuantityUnitsId" />
        <input type="submit" value="حذف" class="btn btn-danger ml-1 mr-1" />
        <a asp-page="./Index">انصراف</a>
    </form>
</div>
