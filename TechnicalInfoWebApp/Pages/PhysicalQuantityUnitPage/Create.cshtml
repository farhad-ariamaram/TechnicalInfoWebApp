@page
@model TechnicalInfoWebApp.Pages.PhysicalQuantityUnitPage.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h1>افزودن</h1>

<h4>واحد کمیت فیزیکی</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TblPhysicalQuantityUnit.FldPhysicalQuantityId" class="control-label">کمیت فیزیکی</label>
                <select asp-for="TblPhysicalQuantityUnit.FldPhysicalQuantityId" class ="form-control" asp-items="ViewBag.FldPhysicalQuantityId"></select>
            </div>
            <div class="form-group">
                <label asp-for="TblPhysicalQuantityUnit.FldUnitId" class="control-label">واحد</label>
                <select asp-for="TblPhysicalQuantityUnit.FldUnitId" class ="form-control" asp-items="ViewBag.FldUnitId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="افزودن" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">بازگشت</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
